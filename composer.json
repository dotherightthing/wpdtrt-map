{
    "name": "dotherightthing/wpdtrt-map",
    "description": "Embed an interactive map.",
    "type": "library",
    "keywords": ["map", "leafletjs", "location", "geotag"],
    "license": "ISC",
    "authors": [
        {
            "name": "Dan Smith",
            "email": "dev@dotherightthing.co.nz",
            "homepage": "https://github.com/dotherightthing/wpdtrt-map",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@dotherightthing.co.nz",
        "issues": "https://github.com/dotherightthing/wpdtrt-map/issues",
        "source": "https://github.com/dotherightthing/wpdtrt-map",
        "wiki": "https://github.com/dotherightthing/wpdtrt-map/wiki"
    },
    "require": {
        "dotherightthing/wpdtrt-plugin-boilerplate": "^1.7.6"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^0.14.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "phpunit/phpunit": "^7.5.14",
        "psy/psysh" : "~0.6",
        "wp-cli/wp-cli": "^2.3",
        "dotherightthing/wpdtrt-exif": "^0.2.2",
        "wpackagist-plugin/advanced-custom-fields": "5.6.*",
        "prairiewest/phpconvertdmstodecimal": "dev-master"
      },
    "extra": {
        "require-wp": [
            {
                "name": "DTRT EXIF",
                "host": "github",
                "repository": "dotherightthing/wpdtrt-exif",
                "file": "wpdtrt-exif.php",
                "description": "Extracts the latitude and longitude information from the featured image, if available."
            },
            {
                "name": "Advanced Custom Fields",
                "host": "wpackagist",
                "repository": "wpackagist-plugin/advanced-custom-fields",
                "file": "acf.php",
                "description": "Provides a map picker for specifying a geocoded location"
            },
            {
                "name": "PHPconvertDMSToDecimal",
                "host": "github",
                "repository": "prairiewest/phpconvertdmstodecimal",
                "file": "convert.php",
                "description": "Used by WPDTRT EXIF to convert geotagging information"
            }
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-plugin-boilerplate"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-debug"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-exif"
        },
        {
            "type": "vcs",
            "url": "https://github.com/prairiewest/phpconvertdmstodecimal"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/tgm-plugin-activation"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config": {
        "vendor-dir": "vendor"
    }
}
